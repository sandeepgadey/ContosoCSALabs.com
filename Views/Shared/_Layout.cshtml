<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Contoso Masks</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" cdnify="true"/>
    <link rel="stylesheet" href="~/css/site.css" cdnify="true"/>
</head>
<body>

    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Contoso
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js" cdnify="true"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" cdnify="true"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" cdnify="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" cdnify="true"></script>
    <script src="~/js/site.js" cdnify="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
